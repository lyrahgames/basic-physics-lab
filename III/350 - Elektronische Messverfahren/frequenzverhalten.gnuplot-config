set terminal epslatex size 15cm,9cm
set output "frequenzverhalten.tex"

set title 'Frequenzverhalten des Digitalmultimeters f√ºr $U(t) = 1V \cdot \sin (\omega t)$'
set xl '$f \ [Hz]$'
set yl '$U \ [V]$'

set format x '10^{%T}'
set format y '%.1f'

set logscale x

set key reverse
set key left bottom
set key width -5
set key height 1
set key Left
set key box

plot 'frequenzverhalten.gnuplot-data' \
	using 1:3 \
	with points pt 12 \
	title 'Digitalmultimeter MX-22' \
	, \
	'' using 1:7 \
	with points pt 13 \
	title 'Oszilloskop HM1508-2'